local SettingTab = Window:CreateTab("Settings", 4483362458) -- Title, Image

local PlayerSection = PlayerTab:CreateSection("Player")

PlayerTab:CreateSlider({
    Name = "Adjust Speed",
    Range = {16, 500},
    Increment = 1,
    Suffix = "Speed",
    CurrentValue = 16,
    Callback = function(Value)
    wait(1)
    wait(1)
    local Walker = true
    wait(0.6)
    wait(1)
    wait(1)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
    repeat until Walker == false
   end,
})

local SettingSection = SettingTab:CreateSection("Settings")

local CloseButton = SettingTab:CreateButton({
   Name = "Close Advancer Z Entirelly",
   Callback = function()
    Rayfield:Destroy()
     end,
})

